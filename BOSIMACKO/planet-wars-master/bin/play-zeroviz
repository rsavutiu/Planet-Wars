#!/bin/sh
rm -f stop
IP=98.247.248.39
while test ! -f stop; do
    echo "Player binary: $2"
    date
    ./tcp/tcp "$IP" 995 "$1" -p honolulu-pass "$2"
    if [ "$3" != "" ]; then
        echo "Player binary: $4"
        date
        ./tcp/tcp "$IP" 995 "$3" -p honolulu-pass "$4"
    else
            sleep $((10+RANDOM%20))
    fi
    if [ "$5" != "" ]; then
        echo "Player binary: $6"
        date
        ./tcp/tcp "$IP" 995 "$5" -p honolulu-pass "$6"
    fi
    if [ "$7" != "" ]; then
        echo "Player binary: $7"
        date
        ./tcp/tcp "$IP" 995 "$7" -p honolulu-pass "$8"
    fi
done
